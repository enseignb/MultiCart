<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="admin-2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="adminhtml.user.edit.tabs">
            <block class="OsmanSorkar\MultiCart\Block\Adminhtml\User\Edit\Tab\Content" name="osmansorkar_attribute_content" />
            <action method="addTabAfter">
                <argument name="name" xsi:type="string">user_section</argument>
                <argument name="block" xsi:type="string">osmansorkar_attribute_content</argument>
